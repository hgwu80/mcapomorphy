<script language="JavaScript">
function changepg(pg, lang, id, ts) {
  self.document.location = 'avibase.jsp?pg=' + pg + (lang != ''?'&lang='+lang:'') + (id != null?'&id=' + id:'') + (ts != null?'&ts=' + ts:'');
}
</script>
