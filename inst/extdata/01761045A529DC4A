<script>

$(document).ready(function() {
   loadTree("01761045", 1);
});

function loadTree(concept, ssver) {

   $.ajax({
       url:"taxon_tree_json.jsp",
       dataType:"json",
       async:true,
       cache:true,
       data:{ "avibaseID": concept, "ssver": ssver },
       success:function(data) {
         $('#tree1').tree({
            data: data,
            autoEscape: false,
            autoOpen: true
         });
         $("[id^=ssvs]").css({background: "#FFFFFF"});
         $("#ssvs" + ssver).css({background: "#417694" });
         $("[id^=ssva]").css({color: "#417694"});
         $("#ssva" + ssver).css({color: "#FFFFFF"});
       }
    });
}

</script>

