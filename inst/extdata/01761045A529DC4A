<script>
   $(document).ready(function(){

     var isNav = false;
     var isHoveringOnLoad = true;

     $("[id^=nav]").hover(
       function() {
         var id = $(this).attr("id").substring(3,5);
         $("[id^=sub]").toggle(false);

         if (!isHoveringOnLoad) {
              isNav = true;
              $("#sub" + id).toggle(true);
         }
         isHoveringOnLoad = false;

       },
       function() {
           var id = $(this).attr("id").substring(3,5);
           isNav = false;
           isHoveringOnLoad = false;
           window.setInterval(function() {
             if(!isNav) $("#sub" + id).toggle(false);
           }, 500);

        }
     );

     $(".AVBnav").click(function() {
        var id = $(this).attr("id");
        id = "#sub" + id.substring(3);
        $(id).toggle();
     });

     $(".AVBnav a").click(function(ev) {
        var id = $(this).closest("div").attr("id");
        id = "#sub" + id.substring(3);
        if (!$(id).is(":visible")) {
          $(id).toggle(true);
          ev.preventDefault();
          ev.stopPropagation();
          return;
        }
     });

	$( "#searchboxsm" ).autocomplete({
		source: "suggest_json.jsp",
		select: function( event, ui ) {
			$("#searchboxsm").val(ui.item.label);
			self.location = 'species.jsp?avibaseid=' + ui.item.value;
		}
	});

   });

</script>
